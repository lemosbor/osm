#Названия полигонов
(amenity=hospital | shop=mall | amenity=university) &  name=* {name '${name}' } [0x1e02 resolution 22 continue] # завод, больница, молл, универ
landuse=industrial &  name=* {name '${name}' } [0x2817 resolution 23] # 
place=square & name=* [0x1e03 resolution 21 continue] # площадь
#amenity=bus_station | (railway=station | railway=halt) & station!=subway  &  name=* [0x1e03 resolution 21] # станция

#building=* & name=* [0x6405 resolution 24] # здание с названием
#office=* & name=* {name '${name} (${office})'} [0x2900 resolution 24] # название контор
#addr:housenumber=* & (name=* | addr:housename=*) [0x2900 resolution 24]
#addr:housenumber=* {name '${addr:housenumber}'} [0x2900 resolution 24] # номер дома. Увеличивает карту на 5 %

<finalize> #прогон точек по дополнительным правилам
# The finalizer section is executed for each element when a rule with an element type matches
name=* { name '${name}' } # если есть назавание, указать название

