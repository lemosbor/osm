sport=* {set sport='${sport|subst:"table_tennis=>наст.теннис"|subst:"footboll=>футбол"|subst:"ice_hockey=>хоккей"|subst:"volleyball=>воллейбол"|subst:"fitness=>фитнес"|subst:"athletics=>тренажеры"|subst:"futsal=>мини-футбол"|subst:"basketball=>баскетбол"|subst:"soccer=>футбол"|subst:"running=>бег"|subst:"hockey=>хоккей"|subst:"multi=>тренажеры"|subst:"chess=>шахматы"|subst:"hockey=>хоккей"}' }

